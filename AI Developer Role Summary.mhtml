</span><span><span class="hljs-comment"># Create graph and namespaces</span></span><span>
g = Graph()
SIO = Namespace(</span><span><span class="hljs-string">"http://example.org/sio#"</span></span><span>)
BFO = Namespace(</span><span><span class="hljs-string">"http://purl.obolibrary.org/obo/BFO_"</span></span><span>)
g.bind(</span><span><span class="hljs-string">"sio"</span></span><span>, SIO)
g.bind(</span><span><span class="hljs-string">"bfo"</span></span><span>, BFO)
g.bind(</span><span><span class="hljs-string">"owl"</span></span><span>, OWL)
g.bind(</span><span><span class="hljs-string">"rdfs"</span></span><span>, RDFS)
g.bind(</span><span><span class="hljs-string">"xsd"</span></span><span>, XSD)

</span><span><span class="hljs-comment"># Helper function to add classes and subclass relationships</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">add_class</span></span><span>(</span><span><span class="hljs-params">name, superclass=<span class="hljs-literal">None</span></span></span><span>, comment=</span><span><span class="hljs-literal">None</span></span><span>):
    class_uri = SIO[name]
    g.add((class_uri, RDF.</span><span><span class="hljs-built_in">type</span></span><span>, OWL.Class))
    </span><span><span class="hljs-keyword">if</span></span><span> superclass:
        g.add((class_uri, RDFS.subClassOf, superclass))
    </span><span><span class="hljs-keyword">if</span></span><span> comment:
        g.add((class_uri, RDFS.comment, </span><span><span class="hljs-type">Literal</span></span><span>(comment)))
    </span><span><span class="hljs-keyword">return</span></span><span> class_uri

</span><span><span class="hljs-comment"># Helper function to add object properties</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">add_object_property</span></span><span>(</span><span><span class="hljs-params">name, domain, range_, comment=<span class="hljs-literal">None</span></span></span><span>):
    prop_uri = SIO[name]
    g.add((prop_uri, RDF.</span><span><span class="hljs-built_in">type</span></span><span>, OWL.ObjectProperty))
    g.add((prop_uri, RDFS.domain, domain))
    g.add((prop_uri, RDFS.</span><span><span class="hljs-built_in">range</span></span><span>, range_))
    </span><span><span class="hljs-keyword">if</span></span><span> comment:
        g.add((prop_uri, RDFS.comment, </span><span><span class="hljs-type">Literal</span></span><span>(comment)))
    </span><span><span class="hljs-keyword">return</span></span><span> prop_uri

</span><span><span class="hljs-comment"># Helper function to add datatype properties</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">add_datatype_property</span></span><span>(</span><span><span class="hljs-params">name, domain, dtype, comment=<span class="hljs-literal">None</span></span></span><span>):
    prop_uri = SIO[name]
    g.add((prop_uri, RDF.</span><span><span class="hljs-built_in">type</span></span><span>, OWL.DatatypeProperty))
    g.add((prop_uri, RDFS.domain, domain))
    g.add((prop_uri, RDFS.</span><span><span class="hljs-built_in">range</span></span><span>, dtype))
    </span><span><span class="hljs-keyword">if</span></span><span> comment:
        g.add((prop_uri, RDFS.comment, </span><span><span class="hljs-type">Literal</span></span><span>(comment)))
    </span><span><span class="hljs-keyword">return</span></span><span> prop_uri

</span><span><span class="hljs-comment"># Core Classes</span></span><span>
Entity = add_class(</span><span><span class="hljs-string">"SIO_Entity"</span></span><span>, BFO[</span><span><span class="hljs-string">"0000001"</span></span><span>])
Trait = add_class(</span><span><span class="hljs-string">"Trait"</span></span><span>, BFO[</span><span><span class="hljs-string">"0000020"</span></span><span>])  </span><span><span class="hljs-comment"># DependentContinuant</span></span><span>
Node = add_class(</span><span><span class="hljs-string">"SemanticNode"</span></span><span>, BFO[</span><span><span class="hljs-string">"0000030"</span></span><span>])  </span><span><span class="hljs-comment"># IndependentContinuant</span></span><span>
Connection = add_class(</span><span><span class="hljs-string">"Connection"</span></span><span>, BFO[</span><span><span class="hljs-string">"0000031"</span></span><span>])  </span><span><span class="hljs-comment"># RelationalQuality</span></span><span>
Context = add_class(</span><span><span class="hljs-string">"SIO_Context"</span></span><span>, BFO[</span><span><span class="hljs-string">"0000140"</span></span><span>])  </span><span><span class="hljs-comment"># Site</span></span><span>

</span><span><span class="hljs-comment"># WOMB BODY classes</span></span><span>
Prenatal = add_class(</span><span><span class="hljs-string">"PrenatalCognitiveStructure"</span></span><span>, Node)
WombEnv = add_class(</span><span><span class="hljs-string">"WombEnvironment"</span></span><span>, Context)
SupportiveMatrix = add_class(</span><span><span class="hljs-string">"SupportiveMatrix"</span></span><span>, BFO[</span><span><span class="hljs-string">"0000040"</span></span><span>])  </span><span><span class="hljs-comment"># MaterialEntity</span></span><span>

</span><span><span class="hljs-comment"># ANACOG 1.0 classes</span></span><span>
GenderIdentity = add_class(</span><span><span class="hljs-string">"GenderIdentity"</span></span><span>, Node)
GenderCluster = add_class(</span><span><span class="hljs-string">"GenderCluster"</span></span><span>, Node)
SuperGender = add_class(</span><span><span class="hljs-string">"SuperGender"</span></span><span>, GenderCluster)
GeoGender = add_class(</span><span><span class="hljs-string">"GeoGender"</span></span><span>, GenderCluster)
